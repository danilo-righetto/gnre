{
  "name": "phxcargo/phxcargo-gnre",
  "description": "Gnre components for phxcargo",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/wbianchini/sped-gnre"
    }
  ],
  "require": {
    "php": ">=7.3",
    "guzzlehttp/guzzle": "^6.3",
    "laravel/framework": "5.7.*",
    "nfephp-org/sped-gnre": "dev-master",
    "nyholm/psr7": "^1.1",
    "php-http/curl-client": "^2.0",
    "php-http/message": "^1.7",
    "vlucas/phpdotenv": "~2.2",
    "ext-json": "*",
    "ext-soap": "*",
    "ext-curl": "*",
    "ext-simplexml": "*",
    "ext-dom": "*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "PhxCargo\\Gnre\\": "src/Gnre/"
    }
  },
  "autoload-dev": {
    "psr-4":{
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "PhxCargo\\Gnre\\GnreServiceProvider"
      ]
    }
  }
}
